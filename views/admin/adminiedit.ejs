        <% if (post) { %>
                                <article>
                                    <section class="bg-black inverted overflow-hidden">
                                        <div class="d-flex flex-column py-15 container min-vh-75 level-3">
                                            <div class="row justify-content-center mt-auto">
                                                <div class="col-lg-10 col-xl-8">
                                                    <span
                                                        class="badge bg-opaque-white text-white rounded-pill mb-2">Featured
                                                        Post</span>
                                                    <h1 class="display-1">Match of the year is comming</h1>
                                                </div>
                                            </div>
                                        </div>
                                        <span
                                            class="background background-gradient-horizontal skrollable skrollable-after"
                                            data-top-top="transform: translateY(0%);"
                                            data-top-bottom="transform: translateY(10%);">
                                        </span>
                                    </section>
                                    <div class="fr-element fr-view" dir="auto" aria-disabled="false" spellcheck="false">

                                        <h1>
                                            <%= post.title %>
                                        </h1>
                                        <p>
                                            <%= post.description %>
                                        </p>
                                        <article class="fs-4  fw-bold mb-0 lh-sm">
                                            <%- post.content%>
                                        </article>
                                        <% } %>
                                            <cite class="fs-sm text-muted">New York Magazine</cite>
                                    </div>
                                </article>
